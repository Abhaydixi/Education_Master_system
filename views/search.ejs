<div class="container " style="margin-top: 110px;">
    <div class="row ">
        <% user && user.forEach((item)=>{  %>
            <div class="col-md-4 mt-3">
                <a href="/besicdetails/<%= item._id %>">
                <img src="<%= item.image.url %>"
                    class="w-100" style="height: 300px;" alt=""></a>
            </div>
            <div class="col-md-8">
                <a href="/besicdetails/<%= item._id %>" style="text-decoration: none;">
                <h3 style="color: #f80;"><%= item.schoolname %></h3></a>
                <p style="font-size: 10px;"><%= item.address %></p>
                <p><%= item.description %></p>
                    <a href="/besicdetails/<%= item._id %>">
                        <button class="btn btn-info">More...</button>
                    </a>
            </div>

     <%   })   %>
       
    </div>
</div>







