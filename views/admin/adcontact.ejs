<%- include('./sidebar.ejs') %>
<%- include('./header.ejs') %>


<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="card w-100">
                    <div class="card-header d-flex justify-content-between">
                        <div>
                            <strong class="card-title  text-secondary">Contact Table</strong>
                        </div>                       
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                            <thead class="bg-secondary text-white">
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone</th>
                                    <th>Message</th>
                                </tr>
                            </thead>
                            <tbody> 
                                <% c.forEach((cont)=>{%>                         
                                    <tr>
                                        <td>
                                           <%= cont.name %>
                                        </td>
                                        <td><%= cont.email %></td>
                                       <td><%= cont.phone %></td>
                                       <td><%= cont.message %></td>
                                    </tr>
                                    <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- .animated -->


</div><!-- .content -->
</div><!-- /#right-panel -->



<%- include('./footer.ejs')  %>